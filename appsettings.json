{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "fronendUrl": "",
  "InternalSecret": "",
  "GatewayWarmup": {
    "Enabled": true,
    "RequestTimeoutSeconds": 22,
    "MaxRetries": 5,
    "RetryDelaySeconds": 4,
    "DefaultPath": "/"
  },
  "Jwt": {
    "Key": "",
    "Issuer": "ControlGastosAPI",
    "Audience": "ControlGastosClients",
    "ExpirationMinutes": "60"
  },
  "ReverseProxy": {
    "Routes": {
      "Auths": {
        "ClusterId": "auths-cluster",
        "Match": {
          "Path": "/Auths/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auths-cluster": {
        "Destinations": {
          "dest1": {
            "Address": ""
          }
        },
        "WarmupPath": "/Auths/ok"
      }
    }
  }
}
